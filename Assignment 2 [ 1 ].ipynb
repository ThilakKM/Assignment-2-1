   ],
   "source": [
    "l =[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]\n",
    "for i in range(0,len(l)-1):\n",
    "    for j in range(0,len(l)-1):\n",
    "        if (l[j][1] > l[j + 1][1]):\n",
    "            mini = l[j]\n",
    "            l[j] = l[j+1]\n",
    "            l[j+1]=mini\n",
    "print(l)"
   ]
  }
